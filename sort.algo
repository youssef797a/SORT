ALGORITHM insertionSort(VAR arr : array_of INTEGER[])  : ARRAY_OF INTEGER
VAR
    i, j, key : INTEGER;
BEGIN
    FOR i FROM 1 TO arr.length-1 STEP 1 DO
        key := arr [i];
        WHILE (j >= 0 AND arr[j] > key) DO
            arr[j + 1] := arr [j];
            j := j -1;
        END_WHILE
        arr[j + 1] := key;
    END_FOR
    RETURN
END